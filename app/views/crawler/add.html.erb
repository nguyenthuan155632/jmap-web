<div class="container container-page" style="margin-top: 50px;"> 
	<h2 style="text-align: center;">Add</h2>
	<form method="post" action="<%= add_path %>">
		<div class="form-group">
			<label for="mapId">mapId</label>
			<input type="text" class="form-control" id="mapId" name="mapId">
		</div>
		<div class="form-group"> 
			<label for="venueName">venueName</label>
			<input type="text" class="form-control" id="venueName" name="venueName">
		</div>
		<div class="form-group">
			<label for="floor">floor</label>
			<input type="text" class="form-control" id="floor" name="floor">
		</div>
		<div class="form-group">
			<label for="typeMap">typeMap</label>
			<input type="text" class="form-control" id="typeMap" name="typeMap">
		</div>
		<div class="form-group">
			<label for="venueFloorMapImageUrl">venueFloorMapImageUrl</label>
			<input type="text" class="form-control" id="venueFloorMapImageUrl" name="venueFloorMapImageUrl">
		</div>
		<div class="form-group">
			<label for="venueFloorMapUrl">venueFloorMapUrl</label>
			<input type="text" class="form-control" id="venueFloorMapUrl" name="venueFloorMapUrl">
		</div>
		<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
		<button type="submit" class="btn btn-primary">Add</button>
	</form>
</div>